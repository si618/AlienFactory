{
  "Application": {
    "AutoStart": true
  },
  "OpenRgbClient": {
    "IpAddress": "127.0.0.1",
    "Port": 6742,
    "Name": "Alien Factory",
    "AutoConnect": true,
    "TimeoutInMilliseconds": 1000
  },
  "Serilog": {
    "Using":  [ "Serilog.Sinks.Console", "Serilog.Sinks.Trace" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      { "Name": "Trace" }
    ],
    "Properties": {
      "Application": "AlienFactory"
    }
  },
  "UserInterface": {
    "DevicesVisible": false,
    "LoggingVisible": false
  }
}